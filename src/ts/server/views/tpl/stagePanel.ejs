<style>
    .inputPanel {
        left: 60px;
        top: 600px;
    }

    .stagePanel {
        background: #ccc;
        width: 100%;
        height: 160px;
    }

    .playerPanel {
        display: inline-flex;
        width: 1000px;
        background: #ccc;
    }

    .playerAvatar {
        width: 100%;
    }

    .pickupPlayer {
        width: 150px;
    }

    .WinPanelButton {
        width: 160px;
        height: 65px;
    }

    .NoticePanel {
        width: 1200px;
    }
</style>
<div class="inputPanel" id="panel">
