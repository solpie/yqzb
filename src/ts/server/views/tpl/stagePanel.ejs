    <!--</div>-->
    <div class="playerPanel">
        player id:
        <% for(var i = 0; i < 8; i++) { %>
        <div class="pickupPlayer">
            <img class="playerAvatar" src="/img/player/p0.png">
            <input id="playerId<%= i %>" class="playerId" type="text" style="width: 75px" value="<%= (18 - i) %>"/>
            <input class="btnQuery" type="button" value="query" data-pos="<%= i %>"/>
            <input class="btnUpdate" type="button" value="update↓↓↓" data-pos="<%= i %>"/><br>
            <input name="mvp" class="playerMvp" type="radio" value="" data-pos="<%= i %>"/>MVP
        </div>
        <% } %>
        <input id="btnUpdateAll" type="button" value="updateAll"/>

    </div>
    <br> Mvp pos:<label id="txtMvpPos">0</label>
    <br>
    <input id="btnShowWin" class="WinPanelButton" type="button" value="Show Win"/>
    <input id="btnHideWin" class="WinPanelButton" type="button" value="Hide Win"/>
    <input id="btnSaveGame" class="WinPanelButton" type="button" value="Save Game"/>
    <input id="btnStraight3" class="WinPanelButton" type="button" value="test straight3" onclick="stagePanel2.fadeInStraight3()"/>
    <div class="NoticePanel">
        小喇叭：
        <br>
        <input id="notice" style="width: 100%" value="一二三四五六七八九十一二三四五六七八九十一二三四五六七八九十一二三四五六七八九十">
        <input id="btnNotice" type="button" value="发送 ctrl+enter">
    </div>
